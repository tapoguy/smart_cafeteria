{
  "expo": {
    "name": "smart_cafeteria_system",
    "slug": "smart_cafeteria_system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/daeyang_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/daeyang_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "daeyangsmartcafeteriasystem"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.daeyang.smartcafeteria"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {}
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0b6cc8de-5036-4e68-afa8-9a79b2dc581c"
      }
    }
  }
}
